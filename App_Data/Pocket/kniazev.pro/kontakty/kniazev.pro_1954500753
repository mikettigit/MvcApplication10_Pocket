<script type="text/javascript">

if (!window.GLOBAL_arMapObjects)
	window.GLOBAL_arMapObjects = {};

function init_MAP_wInW7HV5QW()
{
	if (!window.ymaps)
		return;

	if(typeof window.GLOBAL_arMapObjects['MAP_wInW7HV5QW'] !== "undefined")
		return;

	var node = BX("BX_YMAP_MAP_wInW7HV5QW");
	node.innerHTML = '';

	var map = window.GLOBAL_arMapObjects['MAP_wInW7HV5QW'] = new ymaps.Map(node, {
		center: [56.141634804932, 47.185340661377],
		zoom: 17,
		type: 'yandex#map'
	});

	if (map.behaviors.isEnabled("scrollZoom"))
		map.behaviors.disable("scrollZoom");
	if (map.behaviors.isEnabled("dblClickZoom"))
		map.behaviors.disable("dblClickZoom");
	if (map.behaviors.isEnabled("drag"))
		map.behaviors.disable("drag");
	if (map.behaviors.isEnabled("rightMouseButtonMagnifier"))
		map.behaviors.disable("rightMouseButtonMagnifier");
	if (window.BX_SetPlacemarks_MAP_wInW7HV5QW)
	{
		window.BX_SetPlacemarks_MAP_wInW7HV5QW(map);
	}
}

(function bx_ymaps_waiter(){
	if(typeof ymaps !== 'undefined')
		ymaps.ready(init_MAP_wInW7HV5QW);
	else
		setTimeout(bx_ymaps_waiter, 100);
})();


/* if map inits in hidden block (display:none)
*  after the block showed
*  for properly showing map this function must be called
*/
function BXMapYandexAfterShow(mapId)
{
	if(window.GLOBAL_arMapObjects[mapId] !== undefined)
		window.GLOBAL_arMapObjects[mapId].container.fitToViewport();
}

