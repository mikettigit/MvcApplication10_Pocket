<script>
    var cont = $("#form-container-4");
    var item4 = cont.find(".phone-mask");
    var item24 = cont.find(".name-mask");
    var trueValueItem14 = 0;
    var trueValueItem24 = 0;
    var val = 0;

    function nameMask() {
        item24.inputmask("*{0,150}",{
            definitions: {
                "*": {
                    validator: '[а-яА-Я ]',
                    cardinality: 1
                }
            },
            oncomplete: function () {
                item24.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem14 = 1;
                }

                val = item24.val().length;
            },
            onKeyValidation: function () {
                item24.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item24.val().replace(/[_]/g,'').length;
                if (newVal < val) {
                    item24.removeClass("reviews-form__input-element--true");
                    item24.addClass("reviews-form__input-element--false");
                    trueValueItem14 = 0;
                }

                $("#form-container-4").on("submit", function () {
                    if (trueValueItem14 == 0) {
                        return false;
                    } else {
                        return true;
                    }
                });
            }
        });

        $("#form-container-4").on("submit", function () {
            if (trueValueItem14 == 0) {
                item24.addClass("reviews-form__input-element--false");
                return false;
            } else {
                return true;
            }
        });
    }

    function phoneMask() {
        item4.inputmask("+7 (999) 999-99-99", {
            oncomplete: function () {
                item4.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem24 = 1;
                }

                val = item4.val().length;
            },
            onKeyValidation: function () {
                item4.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item4.val().replace(/[_]/g,'').length;
                if (newVal < val) {
                    item4.removeClass("reviews-form__input-element--true");
                    item4.addClass("reviews-form__input-element--false");
                    trueValueItem24 = 0;
                }

                $("#form-container-4").on("submit", function () {
                    if (trueValueItem24 == 0) {
                        return false;
                    } else {
                        return true;
                    }
                });
            }
        });

        $("#form-container-4").on("submit", function () {
            if (trueValueItem24 == 0) {
                item4.addClass("reviews-form__input-element--false");
                return false;
            } else {
                return true;
            }
        });
    }

