                }

                val201 = item3201.val().length;
            },
            onKeyValidation: function () {
                item3201.removeClass("reviews-form__input-element--true");
                item3201.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item3201.val().replace(/[_]/g,'').length;
                if (newVal < val201) {
                    item3201.removeClass("reviews-form__input-element--true");
                    item3201.addClass("reviews-form__input-element--false");
                    trueValueItem3201 = 0;
                }

                $("#form-container-21").on("submit", function () {
                    if (trueValueItem3201 == 0) {
                        return false;
                    } else {
                        return true;
                    }
                });
            }
        });

        $("#form-container-21").on("submit", function () {
            if (trueValueItem3201 == 0) {
                item3201.addClass("reviews-form__input-element--false");
                return false;
            } else {
                return true;
            }
        });
    }

    function phoneMask21() {
        item201.inputmask("+7 (999) 999-99-99", {
            oncomplete: function () {
                item201.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem1201 = 1;
                }

                val201 = item201.val().length;
            },
            onKeyValidation: function () {
                item201.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item201.val().replace(/[_]/g,'').length;
                if (newVal < val201) {
                    item201.removeClass("reviews-form__input-element--true");
                    item201.addClass("reviews-form__input-element--false");
                    trueValueItem1201 = 0;
                }

                $("#form-container-21").on("submit", function () {
                    if (trueValueItem1201 == 0) {
                        return false;
                    } else {
                        return true;
                    }
                });
            }
        });

        $("#form-container-21").on("submit", function () {
            if (trueValueItem1201 == 0) {
                item201.addClass("reviews-form__input-element--false");
                return false;
            } else {
                return true;
            }
        });
    }

    function nameMask21() {
        item2201.inputmask("*{0,150}",{
            definitions: {
                "*": {
                    validator: '[а-яА-Я ]',
                    cardinality: 1
                }
            },
            oncomplete: function () {
                item2201.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem2201 = 1;
                }

                val201 = item2201.val().length;
            },
            onKeyValidation: function () {
                item2201.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item2201.val().replace(/[_]/g,'').length;
                if (newVal < val201) {
                    item2201.removeClass("reviews-form__input-element--true");
                    item2201.addClass("reviews-form__input-element--false");
                    trueValueItem2201 = 0;
                }

                $("#form-container-21").on("submit", function () {
                    if (trueValueItem2201 == 0) {
                        return false;
                    } else {
                        return true;
                    }
                });
            }
        });

        $("#form-container-21").on("submit", function () {
            if (trueValueItem2201 == 0) {
                item2201.addClass("reviews-form__input-element--false");
                return false;
            } else {
                return true;
            }
        });
    }

    dateMask21();
    phoneMask21();
    nameMask21();
</script>

