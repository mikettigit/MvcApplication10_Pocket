    function nameMask3() {
        item23.inputmask("*{0,150}",{
            definitions: {
                "*": {
                    validator: '[а-яА-Я ]',
                    cardinality: 1
                }
            },
            oncomplete: function () {
                item23.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem13 = 1;
                }

                val3 = item23.val().length;
            },
            onKeyValidation: function () {
                item23.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item23.val().replace(/[_]/g,'').length;
                if (newVal < val3) {
                    item23.removeClass("reviews-form__input-element--true");
