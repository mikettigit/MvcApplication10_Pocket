    var cont20 = $("#form-container-20");
    var item20 = cont20.find(".phone-mask");
    var item220 = cont20.find(".name-mask");
    var item320 = cont20.find(".you-date");
    var trueValueItem120 = 0;
    var trueValueItem220 = 0;
    var trueValueItem320 = 0;
    var val20 = 0;

    function dateMask20() {
        item320.inputmask("99.99.9999", {
            isComplete: function () {

                if (item320.val().replace(/[_]/g,'').length == 10 || item320.val().replace(/[_]/g,'').length == 0) {
                    item320.addClass("reviews-form__input-element--true");

                    if (true) {
                        trueValueItem320 = 1;
                    }

                    val20 = item320.val().length;
                }
            },
            oncomplete: function () {

                item320.addClass("reviews-form__input-element--true");

                if (true) {
                    trueValueItem320 = 1;
                }

                val20 = item320.val().length;
            },
            onKeyValidation: function () {
                item320.removeClass("reviews-form__input-element--true");
                item320.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
                var newVal = item320.val().replace(/[_]/g,'').length;
