(function (window, $) {
  'use strict';

  /**
   * Construct object
   * @param {object} params
   * @constructor
   */
  window.FormGenerator = function (params) {
    this.form = params.formContainer;
    this.ajaxMode = params.ajaxMode;
    this.captchaImg = params.captchaImg;
    this.captchaReload = params.captchaReload;
    this.uri = params.uri;
  }

  /**
   * Init events
   * @return {*}
   */
  FormGenerator.prototype.init = function () {
    this.reloadCaptcha();
    if (this.ajaxMode) {
      this.submitForm();
    }
  }

  /**
   * Event submit form when ajaxMode = true
   * @return {*}
   */
  FormGenerator.prototype.submitForm = function () {
    var self = this;
    $(document).on('submit', this.form, function () {
      $.post(self.uri, $(self.form).serialize(), function (response) {
        $(self.form).replaceWith(response.html);
        self.reloadCaptcha();

        $('[data-init-select]').select2({
          minimumResultsForSearch: -1
        });

        $(".you-date").datepicker({
          autoclose: true,
          language: 'ru',
          orientation: 'bottom'
        }).on('changeDate', function(e) {});

        var cont20 = $("#form-container-20");
        var item320 = cont20.find(".you-date");
        var trueValueItem320 = 0;
        var val20 = 0;

        function dateMask20() {
          item320.inputmask("99.99.9999", {
            isComplete: function () {

              if (item320.val().replace(/[_]/g,'').length == 10 || item320.val().replace(/[_]/g,'').length == 0) {
                item320.addClass("reviews-form__input-element--true");

                if (true) {
                  trueValueItem320 = 1;
                }

                val20 = item320.val().length;
              }
            },
            oncomplete: function () {

              item320.addClass("reviews-form__input-element--true");

              if (true) {
                trueValueItem320 = 1;
              }

              val20 = item320.val().length;
            },
            onKeyValidation: function () {
              item320.removeClass("reviews-form__input-element--true");
              item320.addClass("reviews-form__input-element--false");
            },
            onKeyDown: function () {
              var newVal = item320.val().replace(/[_]/g,'').length;
              if (newVal < val20) {
                item320.removeClass("reviews-form__input-element--true");
                item320.addClass("reviews-form__input-element--false");
                trueValueItem320 = 0;
              }

              $("#form-container-20").on("submit", function () {
                if (trueValueItem320 == 0) {
                  return false;
                } else {
                  return true;
                }
